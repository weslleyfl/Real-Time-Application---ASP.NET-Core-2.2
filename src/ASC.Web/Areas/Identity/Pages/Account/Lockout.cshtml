@page
@model LockoutModel
@{
    ViewData["Title"] = "Locked out";
}

<div class="container">
    <div class="row">
        <div class="col s12 m6 offset-m3">
            <i class="material-icons md-48 md-dark orange600">lock</i>
            <header>
                <div class="card-panel center">
                    <h4>@ViewData["Title"]</h4>
                    <p><a href="/"><i class="material-icons md-48 md-dark orange600">lock</i></a></p>
                    <span>Esta conta foi bloqueada. Tente novamente mais tarde.</span>
                </div>
            </header>
        </div>
    </div>
</div>